fun main(){
    val number = 1 + (10* Math.random()).toInt()
    print("guess a number between 0 and 10 ")
    try {
        val guess= readLine()!!.toInt()
        if(guess==number)
           println("You got it!!")
        else
            println("Wrong guess!,the answer was "+number)
println("Game Over")
}
catch (e: Exception){
    println("enter only numbers!!")
}
}
